</exercise><exercise id="element-874">
<problem id="id46086515">
  <para id="element-413">
    <m:math><m:mtext>P(BB)</m:mtext></m:math> =
  </para>
</problem>

<solution id="id46086539" print-placement="end">
  <para id="element-547">
    <m:math><m:mtext>P(BB)</m:mtext> <m:mo> = </m:mo>
<m:mfrac>
 <m:mn>8</m:mn>
 <m:mn>11</m:mn>
</m:mfrac>
<m:mo> ⋅ </m:mo>
<m:mfrac>

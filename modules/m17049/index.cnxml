			<m:mrow>
				<m:mn>1</m:mn>
			</m:mrow>
			<m:mrow>
				<m:apply>
					<m:conjugate/>
					<m:ci>x</m:ci>
				</m:apply>
			</m:mrow>
		</m:mfrac>
	</m:math>
.</para>

<list id="list-86872534" list-type="enumerated" mark-suffix=".">

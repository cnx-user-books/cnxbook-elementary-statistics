                    <m:mrow>
                      <m:mtext>72</m:mtext>
                      <m:mi>,</m:mi>
                      <m:mfrac>
                        <m:mstyle fontsize="8pt">
                          <m:mrow>
                            <m:mtext>72</m:mtext>
                          </m:mrow>
                        </m:mstyle>
                        <m:mstyle fontsize="8pt">
                          <m:mrow>
                            <m:msqrt>
                              <m:mn>5</m:mn>
                            </m:msqrt>
                          </m:mrow>
                        </m:mstyle>
                      </m:mfrac>
                    </m:mrow>

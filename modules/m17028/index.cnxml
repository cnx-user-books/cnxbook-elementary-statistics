        <colspec colnum="3" colname="c3"/>
        <colspec colnum="4" colname="c4"/>
        <thead>
          <row>
            <entry>Major</entry>
            <entry>&lt; $50,000</entry>
            <entry>$50,000 - $68,999</entry>
            <entry>$69,000 +</entry>
          </row>
          </thead>
          <tbody>
          <row>
            <entry>English</entry>
            <entry>5</entry>
            <entry>20</entry>
            <entry>5</entry>
          </row>
          <row>
            <entry>Engineering</entry>
            <entry>10</entry>
            <entry>30</entry>
            <entry>60</entry>
          </row>
          <row>
